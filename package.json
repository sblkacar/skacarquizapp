{
  "name": "quizapp",
  "version": "0.1.0",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@craco/craco": "^7.1.0",
    "bootstrap": "^5.3.3",
    "chart.js": "^4.4.7",
    "html5-qrcode": "^2.3.8",
    "prop-types": "^15.8.1",
    "qrcode.react": "^4.2.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.7",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.4.0",
    "react-router-dom": "^6.28.1",
    "recharts": "^2.15.0",
    "web-vitals": "^4.2.4"
  },
  "scripts": {
    "start": "craco start",
    "build": "GENERATE_SOURCEMAP=false CI=false craco build",
    "test": "craco test",
    "test:coverage": "craco test --coverage --watchAll=false",
    "test:e2e": "cypress run",
    "test:backend": "jest --config backend/jest.config.js",
    "test:all": "npm run test:coverage && npm run test:backend && npm run test:e2e",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/preset-react": "^7.26.3",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.2",
    "cypress": "^12.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.32.2",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.0.0",
    "prettier": "^2.8.0",
    "react-scripts": "^5.0.1",
    "supertest": "^6.3.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/reportWebVitals.js",
      "!src/setupTests.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 30,
        "branches": 30,
        "functions": 30,
        "lines": 30
      }
    },
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ]
  },
  "proxy": "https://skacarquizapp.vercel.app"
}
